{
    "extends": "tslint:recommended",
    "rules": {
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "max-classes-per-file": [
            false
        ],
        "array-type": [
            true,
            "array"
        ],
        "no-bitwise": false,
        "object-literal-sort-keys": false
    }
}